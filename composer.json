{
    "name": "olssonm/roaring",
    "description": "Roaring.io API",
    "homepage": "https://github.com/olssonm/roaring",
    "license": "MIT",
    "authors": [
        {
            "name": "Marcus Olsson",
            "email": "contact@marcusolsson.me",
            "homepage": "https://marcusolsson.me"
        }
    ],
    "require": {
        "php" : ">=7.1",
        "mashape/unirest-php": "3.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Olssonm\\Roaring\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Olssonm\\Roaring\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.4.x-dev"
        },
        "laravel": {
            "providers": [
                "Olssonm\\Scrive\\ScriveServiceProvider"
            ]
        }
    },
    "suggest": {
        "illuminate/support": "To enable Laravel compatability."
    },
    "minimum-stability": "stable"
}
